{
    "name": "kiboko/kloud",
    "description": "Build and manage your development Docker stack for OroCommerce, OroCRM, OroPlatform and Marello applications",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1",
        "ext-json": "*",
        "composer/semver": "^1.4 || ^2.0 || ^3.0",
        "laravel-zero/phar-updater": "^1.0",
        "psr/log": "^1.1",
        "sebastian/comparator": "^3.0 || ^4.0",
        "sebastian/diff": "^3.0 || ^4.0",
        "symfony/console": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/process": "^5.0",
        "symfony/polyfill-php80": "^1.20",
        "symfony/property-access": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/yaml": "^5.0",
        "splitbrain/php-archive": "^1.1"
    },
    "require-dev": {
        "friends-of-phpspec/phpspec-code-coverage": "*",
        "friendsofphp/php-cs-fixer": "^3.0@dev",
        "monolog/monolog": "^2.0 || ^3.0",
        "phpspec/phpspec": "^6.1 || ^7.0",
        "phpunit/phpunit": "^9.5",
        "adlawson/vfs": "^0.12.1"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Kiboko\\Cloud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\Kiboko\\Cloud\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "phpspec": "vendor/bin/phpspec run --stop-on-failure --no-code-generation",
        "phpunit": "vendor/bin/phpunit test/"
    }
}
